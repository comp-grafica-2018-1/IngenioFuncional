<p id="notice"><%= notice %></p>

<h1>Cotiz Esc Cams</h1>

<table>
  <thead>
    <tr>
      <th>Correo</th>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cotiz_esc_cams.each do |cotiz_esc_cam| %>
      <tr>
        <td><%= cotiz_esc_cam.correo %></td>
        <td><%= cotiz_esc_cam.nombre %></td>
        <td><%= link_to 'Show', cotiz_esc_cam %></td>
        <td><%= link_to 'Edit', edit_cotiz_esc_cam_path(cotiz_esc_cam) %></td>
        <td><%= link_to 'Destroy', cotiz_esc_cam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

