<p id="notice"><%= notice %></p>

<h1>Cotiz Mes Cams</h1>

<table>
  <thead>
    <tr>
      <th>Correo</th>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cotiz_mes_cams.each do |cotiz_mes_cam| %>
      <tr>
        <td><%= cotiz_mes_cam.correo %></td>
        <td><%= cotiz_mes_cam.nombre %></td>
        <td><%= link_to 'Show', cotiz_mes_cam %></td>
        <td><%= link_to 'Edit', edit_cotiz_mes_cam_path(cotiz_mes_cam) %></td>
        <td><%= link_to 'Destroy', cotiz_mes_cam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

